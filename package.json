{
  "name": "mobile-whep",
  "workspaces": [
    "packages/android-client",
    "packages/ios-client",
    "packages/react-native-client",
    "examples/react-native/WhipWhepDemo"
  ],
  "packageManager": "yarn@4.4.1",
  "devDependencies": {
    "@babel/runtime": "^7.25.6",
    "@react-native/eslint-config": "^0.74.85",
    "eslint": "8.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.8.0",
    "prettier": "^3.3.3",
    "typescript-eslint": "^7.16.1"
  },
  "scripts": {
    "lint": "yarn workspaces foreach -A -p run lint",
    "lint:check": "yarn workspaces foreach -A -p run lint:check",
    "build": "yarn workspace @mobile-whep/react-native-client run expo-module build",
    "prepare": "yarn && cd examples/react-native/WhipWhepDemo && expo prebuild && cd ios && pod install",
    "tsc": "yarn workspaces foreach -A -p run tsc"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^5.0.2"
  }
}
